dalManager.add(node)\n      setIndex(index)\n    }\n\n    return () => {\n      modalManager.remove(node)\n      setIndex(0)\n    }\n  }, [isOpen, ref])\n\n  return index\n}\n"},"lineCount":null}},"error":null,"hash":"802d74875e7a8e91dbbf439e36cd6252","cacheData":{"env":{}}}